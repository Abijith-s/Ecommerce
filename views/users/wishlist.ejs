<!-- Wishlist Section Start --><%if(wishlist.length==0){%>
  <div class="row d-flex justify-content-center">
    <div class="card px-4 pt-5  mt-4" style="width: 18rem;">
      <img class="card-img-top" src="data:image/png;base64," alt="Card image cap">
      <div class="card-body">
        <h1 class="card-text">Your Wishlist is Empty</h1>
      </div>
    </div>
  </div>
  <%}else{%>
<div class="section section-margin">
    <div class="container">

        <div class="row">
            <div class="col-12">
                <div class="wishlist-table table-responsive">
                    <table class="table table-bordered">
                        <thead>
                            <tr>
                                <th class="pro-thumbnail">Image</th>
                                <th class="pro-title">Product</th>
                                <th class="pro-price">Price</th>
                                <th class="pro-stock">Stock Status</th>
                                <th class="pro-cart">Add to Cart</th>
                                <th class="pro-remove">Remove</th>
                            </tr>
                        </thead>
                        <tbody>
                            <%for(var item in wishlist){%>
                            <tr>
                                <td class="pro-thumbnail"><a href="#"><img class="img-fluid" style="width: 200px; height: 150" src="/product-images/image1/<%=wishlist[item].wishlistproducts.image1 %>.jpg" alt="Product" /></a></td>
                                <td class="pro-title"><a href="#"><%=wishlist[item].wishlistproducts.productname%></a></td>
                                <td class="pro-price"><span><%=wishlist[item].wishlistproducts.price%></span></td>
                                <td class="pro-stock"><span>In Stock</span></td>
                                <td class="pro-cart"><a onclick="addToCart('<%=wishlist[item].wishlistproducts._id%>')" class="btn btn-dark btn-hover-primary rounded-0">Add to Cart</a></td>
                                <td class="pro-remove"><a onclick="deletItem('<%=wishlist[item].wishlistproducts._id%>','<%=wishlist[item].user%>')"><i class="pe-7s-trash"></i></a></td>
                            </tr>
                        <%}%>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>

    </div>
</div>
<%}%>
<!-- Wishlist Section End -->
<div
          class="cart-update-option d-block d-md-flex justify-content-between"
        >
        
         
          <!-- Cart Update End -->
        </div>

        <script>
            function deletItem(proId, userId) {
              
              swal({
            title: "Are you sure?",
            text: "Once deleted, you will not be able to recover this imaginary file!",
            icon: "warning",
            buttons: true,
            dangerMode: true,
          })
          .then((willDelete) => {
            if (willDelete) {
              $.ajax({
                url: "/delete-wishlist-item",
                data: {
                  proId: proId,
                  userId: userId,
                },
                method: "post",
                success: (response) => {
                  if (response) {
                    window.location.reload();
                  }
                },
                error: () => {},
              });
          
            } 
          });
              
            }
          </script>        
              <script>
                function addToCart(proId){
                   
                    $.ajax({
                        url:'/add-to-cart/'+proId,
                        method:'get',
                        success:(response)=>{
                            console.log(response)
                           if(response.status){
                               
                                let count= document.getElementById("cartcount") 
                                let newCount = parseInt(count.innerHTML)+1
                                count.innerHTML = newCount
                            }else{
                                location.href='/login'
                            }
                        }
                    })
                }
            </script>